{
  "name": "vinnysbeachbar",
  "version": "1.0.0",
  "private": true,
  "description": "Jekyll + Tailwind setup",
  "main": "index.js",
  "scripts": {
    "tw:dev": "tailwindcss -c ./tailwind.config.js -i ./assets/css/tailwind.css -o ./assets/css/output-style.css --watch",
    "tw:build": "tailwindcss -c ./tailwind.config.js -i ./assets/css/tailwind.css -o ./assets/css/output-style.css --minify",
    "jekyll:dev": "bundle exec jekyll serve --livereload --incremental",
    "jekyll:build": "JEKYLL_ENV=production bundle exec jekyll build",
    "dev": "concurrently -k \"npm:tw:dev\" \"npm:jekyll:dev\"",
    "build": "npm run tw:build && npm run jekyll:build",
    "vercel-build": "gem install bundler && bundle install && npm run tw:build && JEKYLL_ENV=production bundle exec jekyll build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.5.0",
    "cssnano": "^5.1.15",
    "postcss": "^8.5.6",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.4.18"
  },
  "dependencies": {
    "playground-elements": "^0.17.1",
    "static-browser-server": "^1.0.5"
  }
}
